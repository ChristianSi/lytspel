{
  "description": "Phonetically respells English-language texts",
  "manifest_version": 2,
  "name": "Lytspel",
  "version": "1.0",
  "homepage_url": "https://github.com/ChristianSi/lytspel",
  "icons": {
    "48": "icons/spellcheck-48.png",
    "96": "icons/spellcheck-96.png"
  },
  "web_accessible_resources": [ "data/lytspel-dict.csv " ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/lytspelify.js"]
    }
  ],
  "background": {
    "scripts": ["src/managedict.js"]
  }
}
